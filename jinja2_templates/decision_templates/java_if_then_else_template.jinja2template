{% if model.choices | length > 0 %}
{% if model.acquire_locks %}
// Request locks.
lockManager.lock({{model.acquire_locks | to_comma_separated_lock_id_list}})
{%- endif %}
{% if model.release_locks %}
// Release locks.
lockManager.unlock({{model.release_locks | to_comma_separated_lock_id_list}})
{%- endif %}
if ({{model.choices[0] | get_guard_statement}}) {
    {{model.choices[0] | indent(4, False)}}
}{% for i in range(1, model | length) %} else if({{model.choices[i] | get_guard_statement}}) {
    {{model.choices[i] | indent(4, False)}}
}
{%- endfor %}
return false;
{%- endif %}